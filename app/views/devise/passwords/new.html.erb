<div class="home-video-wrapper">
  <%=video_tag "GMU.mp4", class:"home_video",autoplay: :autoplay,loop: :loop, muted: :muted %>
  <div class="container">
    <div class="row mt-5">
      <div class="col-md-4"></div>
      <div class="col-md-4 mt-5 form-group", text-align: "center">
      <fieldset class="form">
        <h2>Forgot your password?</h2>
        <%= form_for(resource, as: resource_name, url: password_path(resource_name), html: { method: :post }) do |f| %>
          <%= render "devise/shared/error_messages", resource: resource %>

          <div class="field">
            <%= f.label :email %><br />
            <%= f.email_field :email, autofocus: true, autocomplete: "email" %> <br />
          </div><br>

          <div class="actions">
            <%= f.submit "Email Reset Info" ,class: "btn btn-primary" %> <br/ >
          </div><br>
        <% end %>

        <%= render "devise/shared/links" %>
        </fieldset>
        </div>
    </div>
  </div>
</div>